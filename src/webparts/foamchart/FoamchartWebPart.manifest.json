{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx/client-side-web-part-manifest.schema.json",
  "id": "290bcf6b-f0e2-4cb1-9c98-ede87ed18834",
  "alias": "FoamchartWebPart",
  "componentType": "WebPart",

  // The "*" signifies that the version should be taken from the package.json
  "version": "*",
  "manifestVersion": 2,

  // If true, the component can only be installed on sites where Custom Script is allowed.
  // Components that allow authors to embed arbitrary script code should set this to true.
  // https://support.office.com/en-us/article/Turn-scripting-capabilities-on-or-off-1f2c515f-5d7e-448a-9fd7-835da935584f
  "requiresCustomScript": false,
  "supportedHosts": ["SharePointWebPart"],

  "preconfiguredEntries": [{
      "groupId": "5c03119e-3074-46fd-976b-c60198311f70", // Other
      "group": { "default": "Other" },
      "title": { "default": "foamchart-TMT (Beta Testing)" },
      "description": { "default": "Show list data as HeatMap of TMT by day of week.  Mimics Github Contributor chart" },
      "officeFabricIconFontName": "WebAppBuilderFragment",
      "properties": {
        "description": "foamchart-TMT",
        "parentListWeb": "/sites/Templates/Tmt/",
        "parentListTitle": "TrackMyTime",
  
        "dateColumn": "StartTime",
        "monthGap": "1",
        "squareCustom": "transparent,#ebedf0,#c6e48b,#7bc96f,#196127",
        "cellColor": "green",
  
        "valueColumn": "Hours",
        "valueType": "Number",
        "valueOperator": "Sum",
        "minDataDownload": true,
        "showEarlyAccess": true,
        "scaleMethod": "blink",
        "fetchCount": 2000,
        "enableSearch": true,
        "dropDownColumns": "+Story,+>Chapter,+User/Title"
      }
    },
    {
      "groupId": "5c03119e-3074-46fd-976b-c60198311f70", // Other
      "group": { "default": "Other" },
      "title": { "default": "foamchart-Documents (Beta Testing)" },
      "description": { "default": "Show list data as HeatMap Documents by day of week.  Mimics Github Contributor chart" },
      "officeFabricIconFontName": "WebAppBuilderFragment",
      "properties": {
        "description": "foamchart-Documents",
        "parentListWeb": "",
        "parentListTitle": "Documents",
  
        "dateColumn": "Modified",
        "monthGap": "1",
        "squareCustom": "transparent,#ebedf0,#c6e48b,#7bc96f,#196127",
        "cellColor": "green",
  
        "valueColumn": "Id",
        "valueType": "Number",
        "valueOperator": "Count",
        "minDataDownload": true,
        "showEarlyAccess": true,
        "scaleMethod": "blink",
        "fetchCount": 2000,
        "enableSearch": true,
        "dropDownColumns": "+Author/Title,+Editor/Title"
      }
    },
    {
      "groupId": "5c03119e-3074-46fd-976b-c60198311f70", // Other
      "group": { "default": "Other" },
      "title": { "default": "foamchart-ACH Turnover (Beta Testing)" },
