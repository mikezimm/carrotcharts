        "parentListTitle": "ACH Turnover",
  
        "dateColumn": "zzzDate1",
        "monthGap": "1",
        "squareCustom": "transparent,#ebedf0,#c6e48b,#7bc96f,#196127",
        "cellColor": "green",
  
        "valueColumn": "zzzNumber05",
        "valueType": "Number",
        "valueOperator": "Sum",
        "minDataDownload": true,
        "showEarlyAccess": true,
        "scaleMethod": "blink",
        "fetchCount": 5000,
        "enableSearch": true,
        "dropDownColumns": "+zzzChoice1,+zzzItemCategory"
      }
